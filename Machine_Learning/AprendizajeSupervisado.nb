Cell[CellGroupData[{

Cell["Algoritmo SGD", "Subchapter",
 CellChangeTimes->{{3.660785560940379*^9, 3.660785564108201*^9}}],

Cell[TextData[{
 "El algoritmo SGD (Stochastic gradient descent) consiste en actualizar los \
valores de \[Theta] con cada ejemplo de entrenamiento.  Con esta variaci\
\[OAcute]n el algoritmo es\n\nLoop[\n\tPara i=1 hasta m [\n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Theta]", "j"], " ", "=", " ", 
    RowBox[{
     SubscriptBox["\[Theta]", "j"], " ", "+", 
     RowBox[{"\[Alpha]", " ", 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["y", 
         RowBox[{"(", "i", ")"}]], "-", 
        RowBox[{
         SubscriptBox["h", "\[Theta]"], "(", 
         SuperscriptBox["x", 
          RowBox[{"(", "i", ")"}]], ")"}]}], ")"}], " ", 
      SubsuperscriptBox["x", "j", 
       RowBox[{"(", "i", ")"}]]}]}]}], TraditionalForm]]],
 "  (para cada j)\n\t]\n]\n\nA continuaci",
 "\[OAcute]n se muestra un ejemplo del algoritmo."
}], "Text",
 CellChangeTimes->{{3.660786462183114*^9, 3.6607864860388603`*^9}, {
  3.660786631388709*^9, 3.660786646396061*^9}, {3.660786818217751*^9, 
  3.660786920704095*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"X", " ", "=", " ", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"1", ",", 
       RowBox[{"d", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"d", ",", "data"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"y", " ", "=", " ", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"d", "[", 
       RowBox[{"[", "2", "]"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"d", ",", "data"}], "}"}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
    "\[Theta]1", ",", "\[Theta]2", ",", "\[Alpha]", ",", "\[CapitalTheta]"}], 
    "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Theta]1", " ", "=", " ", 
     RowBox[{"RandomReal", "[", "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"\[Theta]2", " ", "=", " ", 
     RowBox[{"RandomReal", "[", "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"\[Alpha]", " ", "=", " ", "0.01"}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"\[CapitalTheta]", "[", 
      RowBox[{"\[Theta]1c_", ",", "\[Theta]2c_"}], "]"}], " ", ":=", " ", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", "\[Theta]1", "}"}], ",", 
       RowBox[{"{", "\[Theta]2", "}"}]}], "}"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"hipotesis", "[", 
      RowBox[{"X_", ",", "\[CapitalTheta]_"}], "]"}], " ", ":=", " ", 
     RowBox[{
      RowBox[{"Flatten", "[", 
       RowBox[{
        RowBox[{"Transpose", "[", "\[CapitalTheta]", "]"}], ".", "X"}], "]"}],
       "[", 
      RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Cost", "[", 
      RowBox[{"X_", ",", "y_", ",", "\[Theta]_"}], "]"}], " ", ":=", " ", 
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"Sum", "[", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           RowBox[{"hipotesis", "[", 
            RowBox[{
             RowBox[{"X", "[", 
              RowBox[{"[", "i", "]"}], "]"}], ",", "\[Theta]"}], "]"}], " ", 
           "-", " ", 
           RowBox[{"y", "[", 
            RowBox[{"[", "i", "]"}], "]"}]}], ")"}], "2"], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"Length", "[", "y", "]"}]}], "}"}]}], "]"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"costs", " ", "=", " ", 
     RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Do", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"Do", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"\[Theta]1", "  ", "+=", " ", 
           RowBox[{"\[Alpha]", " ", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"y", "[", 
               RowBox[{"[", "i", "]"}], "]"}], " ", "-", " ", 
              RowBox[{"hipotesis", "[", 
               RowBox[{
                RowBox[{"X", "[", 
                 RowBox[{"[", "i", "]"}], "]"}], ",", 
                RowBox[{"\[CapitalTheta]", "[", 
                 RowBox[{"\[Theta]1", ",", "\[Theta]2"}], "]"}]}], "]"}]}], 
             ")"}], " ", 
            RowBox[{
             RowBox[{"X", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "[", 
             RowBox[{"[", "1", "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"\[Theta]2", "  ", "+=", " ", 
           RowBox[{"\[Alpha]", " ", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"y", "[", 
               RowBox[{"[", "i", "]"}], "]"}], " ", "-", " ", 
              RowBox[{"hipotesis", "[", 
               RowBox[{
                RowBox[{"X", "[", 
                 RowBox[{"[", "i", "]"}], "]"}], ",", 
                RowBox[{"\[CapitalTheta]", "[", 
                 RowBox[{"\[Theta]1", ",", "\[Theta]2"}], "]"}]}], "]"}]}], 
             ")"}], " ", 
            RowBox[{
             RowBox[{"X", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "[", 
             RowBox[{"[", "2", "]"}], "]"}]}]}], ";"}], "\[IndentingNewLine]",
          ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", 
           RowBox[{"Length", "[", "y", "]"}]}], "}"}]}], 
        "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"AppendTo", "[", 
        RowBox[{"costs", ",", 
         RowBox[{"Cost", "[", 
          RowBox[{"X", ",", "y", ",", 
           RowBox[{"\[CapitalTheta]", "[", 
            RowBox[{"\[Theta]1", ",", "\[Theta]2"}], "]"}]}], "]"}]}], "]"}], 
       ";"}], "\[IndentingNewLine]", ",", 
      RowBox[{"{", "150", "}"}]}], "\[IndentingNewLine]", "]"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Print", "[", 
     RowBox[{"\"\<El modelo que se encontr\[OAcute] es: (\!\(\*GridBox[{
{\[Theta]0},
{\[Theta]1}
}]\)) = \>\"", ",", " ", 
      RowBox[{"MatrixForm", "[", 
       RowBox[{"\[CapitalTheta]", "[", 
        RowBox[{"\[Theta]1", ",", "\[Theta]2"}], "]"}], "]"}]}], "]"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Print", "[", 
     RowBox[{"ListPlot", "[", 
      RowBox[{"costs", ",", 
       RowBox[{"PlotRange", "\[Rule]", "Full"}], ",", " ", 
       RowBox[{"PlotTheme", "\[Rule]", "\"\<Scientific\>\""}], ",", " ", 
       RowBox[{"FrameLabel", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"\"\<Iteraci\[OAcute]n\>\"", ",", "\"\<Costo\>\""}], "}"}]}],
        ",", " ", 
       RowBox[{
       "PlotLabel", "\[Rule]", "\"\<Costo respecto a iteraciones\>\""}]}], 
      "]"}], "]"}], ";"}]}], "\[IndentingNewLine]", "]"}]}], "Input",
