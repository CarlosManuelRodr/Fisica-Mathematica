             RowBox[{"Count", "[", 
              RowBox[{
               RowBox[{"T", "[", 
                RowBox[{"All", ",", "X"}], "]"}], ",", "#"}], "]"}], 
             RowBox[{"Length", "[", 
              RowBox[{"T", "[", 
               RowBox[{"All", ",", "X"}], "]"}], "]"}]], "*", 
            RowBox[{"CalcEntropy", "[", 
             RowBox[{"grouped", "[", 
              RowBox[{"#", ",", "All", ",", "Cl"}], "]"}], "]"}]}], "]"}], 
          "&"}], ",", 
         RowBox[{"Keys", "[", "grouped", "]"}]}], "]"}], "]"}]}]}], 
