      "\[IndentingNewLine]", 
      RowBox[{"subX", " ", "=", " ", 
       RowBox[{
        RowBox[{"TakeLargestBy", "[", 
         RowBox[{"gains", ",", "Last", ",", "1"}], "]"}], "[", 
        RowBox[{"1", ",", "1"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"branches", " ", "=", " ", 
       RowBox[{"T", "[", 
        RowBox[{"GroupBy", "[", "subX", "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"Map", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"subX", ",", "#", ",", 
