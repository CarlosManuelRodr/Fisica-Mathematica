       RowBox[{"T", "[", 
        RowBox[{"GroupBy", "[", "X", "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Total", "[", 
       RowBox[{"Map", "[", 
