           RowBox[{"All", ",", "Cl"}], "]"}], "]"}], "\[Equal]", " ", "1"}], 
        ",", " ", 
        RowBox[{"Return", "[", 
         RowBox[{"StringJoin", "[", 
          RowBox[{"Cl", ",", "\"\<: \>\"", ",", 
           RowBox[{"First", "[", 
            RowBox[{"DeleteDuplicates", "[", 
             RowBox[{"T", "[", 
              RowBox[{"All", ",", "Cl"}], "]"}], "]"}], "]"}]}], "]"}], 
         "]"}]}], "]"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"gains", " ", "=", " ", 
       RowBox[{"Dataset", "[", 
        RowBox[{"AttributesInformationGain", "[", 
