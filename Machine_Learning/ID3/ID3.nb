   RowBox[{"PartialEntropy", "[", 
    RowBox[{"X_", ",", "T_", ",", "Cl_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", "grouped", "}"}], ",", "\[IndentingNewLine]", 
