                "]"}], ",", "#"}], "}"}], "&"}], ",", "rest"}], "]"}]}], 
         "\[IndentingNewLine]", "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"vertex", " ", "=", " ", 
       RowBox[{"ReplaceRepeated", "[", 
        RowBox[{"nestedList", ",", "rule"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"DeleteDuplicates", "[", 
