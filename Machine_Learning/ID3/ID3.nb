  RowBox[{
   RowBox[{"ID3", "[", 
    RowBox[{"X_", ",", "T_", ",", "Cl_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
