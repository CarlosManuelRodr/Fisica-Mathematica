Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"BranchSwitch", "[", 
    RowBox[{"data_", ",", "tree_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"switch", ",", "vertex", ",", "branches", ",", "subVertex"}], 
      "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Length", "[", "tree", "]"}], ">", "0"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"vertex", " ", "=", " ", 
          RowBox[{"First", "[", 
           RowBox[{"tree", "[", 
            RowBox[{"[", 
             RowBox[{"All", ",", "1"}], "]"}], "]"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"branches", " ", "=", " ", 
          RowBox[{"tree", "[", 
           RowBox[{"[", 
            RowBox[{"All", ",", "2"}], "]"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"subVertex", " ", "=", " ", 
          RowBox[{"Map", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"BranchSwitch", "[", 
              RowBox[{"data", ",", "#"}], "]"}], "&"}], ",", 
            RowBox[{"tree", "[", 
             RowBox[{"[", 
              RowBox[{"All", ",", "3"}], "]"}], "]"}]}], "]"}]}], ";", 
         "\[IndentingNewLine]", "\[IndentingNewLine]", 
         RowBox[{"switch", " ", "=", " ", 
          RowBox[{"Apply", "[", 
           RowBox[{"Switch", ",", 
            RowBox[{"Join", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"data", "[", "vertex", "]"}], "}"}], ",", 
              RowBox[{"Flatten", "[", 
               RowBox[{
                RowBox[{"Transpose", "[", 
                 RowBox[{"{", 
                  RowBox[{"branches", ",", "subVertex"}], "}"}], "]"}], ",", 
                "1"}], "]"}]}], "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
         
         RowBox[{"Return", "[", "switch", "]"}], ";"}], "\[IndentingNewLine]",
         ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Return", "[", "tree", "]"}], ";"}]}], "\[IndentingNewLine]",
        "]"}], ";"}]}], "\[IndentingNewLine]", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ID3Prediction", "[", 
    RowBox[{"data_", ",", "tree_", ",", "predict_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"prediction", ",", "deleteStr"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"prediction", " ", "=", " ", 
       RowBox[{"BranchSwitch", "[", 
        RowBox[{"data", ",", "tree"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"deleteStr", " ", "=", " ", 
       RowBox[{"StringJoin", "[", 
        RowBox[{"predict", ",", "\"\<: \>\""}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"StringDelete", "[", 
        RowBox[{"prediction", ",", "deleteStr"}], "]"}], "]"}], ";"}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"SucessRateID3", "[", 
    RowBox[{"dataset_", ",", "tree_", ",", "predict_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"dataPredict", ",", "modelPredict", ",", "matches"}], "}"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"dataPredict", " ", "=", " ", 
       RowBox[{"Normal", "[", 
        RowBox[{"dataset", "[", 
         RowBox[{"All", ",", "predict"}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"modelPredict", " ", "=", " ", 
       RowBox[{"Normal", "[", 
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"ID3Prediction", "[", 
            RowBox[{"#", ",", "tree", ",", "predict"}], "]"}], "&"}], ",", 
          "dataset"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"matches", " ", "=", " ", 
       RowBox[{"Count", "[", 
        RowBox[{
         RowBox[{"MapThread", "[", 
          RowBox[{"SameQ", ",", 
           RowBox[{"{", 
            RowBox[{"dataPredict", ",", "modelPredict"}], "}"}]}], "]"}], ",",
          "True"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"100", "*", 
        FractionBox["matches", 
         RowBox[{"Length", "[", "dataPredict", "]"}]]}], "]"}], ";"}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]}], "Input",
