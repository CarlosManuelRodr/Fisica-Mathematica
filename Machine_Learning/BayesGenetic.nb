Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"CheckComplex", "[", "l_", "]"}], ":=", 
   RowBox[{"Apply", "[", 
    RowBox[{"Or", ",", 
     RowBox[{"Map", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"MatchQ", "[", 
         RowBox[{"#", ",", "_Complex"}], "]"}], "&"}], ",", "l"}], "]"}]}], 
    "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"CheckNegative", "[", "l_", "]"}], ":=", 
   RowBox[{"Apply", "[", 
    RowBox[{"Or", ",", 
     RowBox[{"Map", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"Re", "[", "#", "]"}], "\[LessEqual]", "0"}], "&"}], ",", 
       "l"}], "]"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"IsDAG", "[", "matrix_", "]"}], ":=", 
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", "ev", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Tr", "[", "matrix", "]"}], "\[NotEqual]", "0"}], ",", 
         RowBox[{"Return", "[", "False", "]"}]}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ev", " ", "=", " ", 
        RowBox[{"N", "[", 
         RowBox[{"Eigenvalues", "[", 
          RowBox[{"matrix", "+", 
           RowBox[{"IdentityMatrix", "[", 
            RowBox[{"Length", "[", "matrix", "]"}], "]"}]}], "]"}], "]"}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"CheckNegative", "[", "ev", "]"}], " ", "||", " ", 
          RowBox[{"CheckComplex", "[", "ev", "]"}]}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"Return", "[", "False", "]"}], ",", "\[IndentingNewLine]", 
         RowBox[{"Return", "[", "True", "]"}]}], "\[IndentingNewLine]", "]"}],
        ";"}]}], "\[IndentingNewLine]", "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"RemoveRandomVertex", "[", "matrix_", "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"vector", ",", "removedIndex"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"vector", " ", "=", " ", 
       RowBox[{"Flatten", "[", "matrix", "]"}]}], ";", "\[IndentingNewLine]", 
      
      RowBox[{"removedIndex", " ", "=", " ", 
       RowBox[{"RandomChoice", "[", 
        RowBox[{"Position", "[", 
         RowBox[{"vector", ",", "1"}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Partition", "[", 
       RowBox[{
        RowBox[{"ReplacePart", "[", 
         RowBox[{"vector", ",", 
          RowBox[{
           RowBox[{"First", "[", "removedIndex", "]"}], "\[Rule]", "0"}]}], 
         "]"}], ",", 
        RowBox[{"Length", "[", "matrix", "]"}]}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"SimplifyToDAG", "[", "matrix_", "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"IsDAG", "[", "matrix", "]"}], ",", "\[IndentingNewLine]", 
       "matrix", "\[IndentingNewLine]", ",", "\[IndentingNewLine]", 
       RowBox[{"SimplifyToDAG", "[", 
        RowBox[{"RemoveRandomVertex", "[", "matrix", "]"}], "]"}]}], 
      "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", "]"}]}], 
  ";"}]}], "Input",
 InitializationCell->True,
