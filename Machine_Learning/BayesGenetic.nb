Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"RemoveDiagonal", "[", "matrix_", "]"}], ":=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"i", " ", "\[NotEqual]", " ", "j"}], ",", 
       RowBox[{"matrix", "[", 
        RowBox[{"[", 
         RowBox[{"i", ",", "j"}], "]"}], "]"}], ",", "0"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"Length", "[", "matrix", "]"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "1", ",", 
       RowBox[{"Length", "[", 
        RowBox[{"First", "[", "matrix", "]"}], "]"}]}], "}"}]}], "]"}]}], 
  ";"}]], "Input",
 InitializationCell->
  True,ExpressionUUID->"1758f139-3b73-4dde-a18c-db9dd2886995"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"BuildMatrix", "[", 
    RowBox[{"paramList_", ",", "n_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", "parts", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"parts", " ", "=", " ", 
       RowBox[{"Partition", "[", 
        RowBox[{"paramList", ",", 
         RowBox[{"n", "-", "1"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"RemoveDiagonal", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Insert", "[", 
          RowBox[{
           RowBox[{"parts", "[", 
            RowBox[{"[", "i", "]"}], "]"}], ",", "1", ",", "i"}], "]"}], ",", 
         
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "]"}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.784560230490562*^9, 3.784560233237708*^9}},
 CellLabel->"In[41]:=",ExpressionUUID->"e209f3be-88c0-474a-acaf-b727933aafe8"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"GeneratePopulationSeed", "[", 
    RowBox[{"n_", ",", "size_"}], "]"}], ":=", 
   RowBox[{"Map", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"BuildMatrix", "[", 
       RowBox[{"#", ",", "n"}], "]"}], "&"}], ",", 
     RowBox[{"RandomSample", "[", 
      RowBox[{
       RowBox[{"Tuples", "[", 
        RowBox[{
