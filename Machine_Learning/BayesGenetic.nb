Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"GeneratePopulation", "[", 
    RowBox[{"n_", ",", "size_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"populationSeed", ",", "dag"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"populationSeed", " ", "=", " ", 
       RowBox[{"GeneratePopulationSeed", "[", 
        RowBox[{"n", ",", "size"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"dag", " ", "=", " ", 
       RowBox[{"Map", "[", 
        RowBox[{"SimplifyToDAG", ",", "populationSeed"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "dag", "]"}], ";"}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}]], "Input",
