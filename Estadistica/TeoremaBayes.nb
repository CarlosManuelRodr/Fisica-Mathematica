      RowBox[{"groupSickIndexes", " ", "=", " ", 
       RowBox[{"Position", "[", 
        RowBox[{"group", ",", "1"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"array", " ", "=", " ", 
       RowBox[{"ReplacePart", "[", 
        RowBox[{"array", ",", 
         RowBox[{"groupSickIndexes", "\[Rule]", "1"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "array", "]"}], ";"}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
