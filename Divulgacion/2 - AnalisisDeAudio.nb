  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"bc", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"u", "[", 
       RowBox[{"0", ",", "t"}], "]"}], "\[Equal]", "0"}], ",", 
     RowBox[{
      RowBox[{"u", "[", 
       RowBox[{"\[Pi]", ",", "t"}], "]"}], "\[Equal]", "0"}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ic", " ", "=", " ", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"u", "[", 
       RowBox[{"x", ",", " ", "0"}], "]"}], " ", "==", " ", 
      RowBox[{
       RowBox[{"x", "^", "2"}], " ", 
       RowBox[{"(", 
        RowBox[{"\[Pi]", " ", "-", " ", "x"}], ")"}]}]}], ",", " ", 
     RowBox[{
      RowBox[{
       SuperscriptBox["u", 
        TagBox[
         RowBox[{"(", 
          RowBox[{"0", ",", "1"}], ")"}],
         Derivative],
        MultilineFunction->None], "[", 
       RowBox[{"x", ",", " ", "0"}], "]"}], " ", "==", " ", "0"}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"dsol", "=", 
  RowBox[{
   RowBox[{"DSolve", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"weqn", ",", "bc", ",", "ic"}], "}"}], ",", "u", ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", "t"}], "}"}]}], "]"}], "/.", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"K", "[", "1", "]"}], "\[Rule]", "m"}], 
    "}"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"asol", "[", 
    RowBox[{"x_", ",", "t_"}], "]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"u", "[", 
       RowBox[{"x", ",", "t"}], "]"}], "/.", 
      RowBox[{"dsol", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], "/.", 
     RowBox[{"{", 
      RowBox[{"\[Infinity]", "\[Rule]", "10"}], "}"}]}], "//", "Activate"}]}],
   ";"}]}], "Input",ExpressionUUID->"3673bb61-9940-4dd3-972e-3ad57acf8cab"],

