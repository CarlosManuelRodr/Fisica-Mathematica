   RowBox[{"Partition", "[", 
    RowBox[{"data", ",", " ", 
     RowBox[{"samplerate", "/", "2"}]}], "]"}]}], ";"}]], "Input",ExpressionUU\
ID->"a525654b-f29b-4ef4-8e26-ee9a02691eb9"],

