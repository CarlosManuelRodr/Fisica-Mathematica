Cell["\<\
C\[AAcute]lculo del exponente de Lyapunov para los n\[UAcute]meros generados \
aleatoriamente.\
\>", "Text",
 CellChangeTimes->{{3.6656984094110804`*^9, 
  3.665698418879936*^9}},ExpressionUUID->"d980b1d4-ae3c-4ffb-a8ef-\
1138be143d15"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"LyapunovLCG", "[", 
     RowBox[{"param_", ",", "x0_", ",", "iter_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "n", ",", "pts", ",", "x", ",", "xeps", ",", "sum", ",", "d", ",", 
        "eps"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"eps", " ", "=", " ", "1"}], ";", "\[IndentingNewLine]", 
       RowBox[{"n", " ", "=", " ", "iter"}], ";", "\[IndentingNewLine]", 
       RowBox[{"x", " ", "=", " ", "x0"}], ";", "\[IndentingNewLine]", 
       RowBox[{"xeps", " ", "=", " ", 
        RowBox[{"x", "-", "eps"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"sum", " ", "=", " ", "0"}], ";", "\[IndentingNewLine]", 
       RowBox[{"Do", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"x", " ", "=", " ", 
           RowBox[{"LCG", "[", 
            RowBox[{"x", ",", "param", ",", "0", ",", 
             SuperscriptBox["2", "31"]}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"xeps", " ", "=", " ", 
           RowBox[{"LCG", "[", 
            RowBox[{"xeps", ",", "param", ",", "0", ",", 
             SuperscriptBox["2", "31"]}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"d", " ", "=", " ", 
           RowBox[{"Abs", "[", 
            RowBox[{"x", "-", "xeps"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"sum", " ", "+=", " ", 
           RowBox[{"Log", "[", 
            RowBox[{"d", "/", "eps"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"xeps", " ", "=", " ", 
           RowBox[{"x", "-", "eps"}]}], ";"}], ",", "\[IndentingNewLine]", 
         RowBox[{"{", "iter", "}"}]}], "\[IndentingNewLine]", "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Return", "[", 
        RowBox[{"N", "[", 
         RowBox[{"sum", "/", "iter"}], "]"}], "]"}], ";"}]}], 
     "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]"}]], "Input",
 CellChangeTimes->{{3.6652608238454924`*^9, 3.6652608392588215`*^9}, {
  3.6652608708543158`*^9, 3.6652608789950237`*^9}, {3.66526091256207*^9, 
