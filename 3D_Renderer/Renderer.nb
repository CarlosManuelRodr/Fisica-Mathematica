           RowBox[{
            RowBox[{"height", "-", "1"}], ",", 
            RowBox[{"Max", "[", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"p1", "[", 
                RowBox[{"[", "2", "]"}], "]"}], ",", 
               RowBox[{"p2", "[", 
                RowBox[{"[", "2", "]"}], "]"}], ",", 
               RowBox[{"p3", "[", 
                RowBox[{"[", "2", "]"}], "]"}]}], "}"}], "]"}]}], "}"}], 
          "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
