             RowBox[{"[", "1", "]"}], "]"}]}]}], "}"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"v2", " ", "=", " ", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"C", "[", 
             RowBox[{"[", "2", "]"}], "]"}], "-", 
            RowBox[{"A", "[", 
             RowBox[{"[", "2", "]"}], "]"}]}], ",", 
           RowBox[{
            RowBox[{"B", "[", 
             RowBox[{"[", "2", "]"}], "]"}], "-", 
            RowBox[{"A", "[", 
             RowBox[{"[", "2", "]"}], "]"}]}], ",", 
           RowBox[{
            RowBox[{"A", "[", 
             RowBox[{"[", "2", "]"}], "]"}], "-", 
            RowBox[{"P", "[", 
             RowBox[{"[", "2", "]"}], "]"}]}]}], "}"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"u", " ", "=", " ", 
         RowBox[{"Cross", "[", 
          RowBox[{"v1", ",", "v2"}], "]"}]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
